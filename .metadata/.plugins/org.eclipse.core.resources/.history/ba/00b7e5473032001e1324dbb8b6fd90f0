#include<stdio.h>
#include<string.h>
bool strcomp(char 'a',char 'b')
{
	int i=0;
	while(a[i]==b[i] && a[i]=='\0' && b[i]=='\0')
	{
		i=i+1;
	}
	if(a[i]=='\0' && b[i]=='\0')
	{
		return true;
	}
	else
	{
		return false;
	}
}
int main()
{
	char username="Subrat@2023"
	char userid[20];
	printf("Enter username: ");
	scanf("%s",&userid);
	while(strcmp(userid,username)!=0)
	{
		printf("Enter username: ");
		scanf("%s",&userid);
		if(strcmp(userid,username)==0)
			{
				printf("Welcome!!");
			}
			else
			{
				printf("Enter password again: ");
			}
	}
}



